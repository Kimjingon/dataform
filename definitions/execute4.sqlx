config {
    type: "table",
    name: "final_table"
}

SELECT
  t1.id AS id,
  t1.name1 AS name1,
  t2.name2 AS name2,
  t3.name3 AS name3,
  t1.CALL_COUNT AS c1,
  t2.CALL_COUNT AS c2,
  t3.CALL_COUNT AS c3
FROM
  ${ref("count_table1")} AS t1
LEFT JOIN
  ${ref("count_table2")} AS t2
ON
  t1.id = t2.id
LEFT JOIN
  ${ref("count_table3")} AS t3
ON
  t1.id = t3.id
